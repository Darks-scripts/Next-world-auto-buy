local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local MainTab = Window:MakeTab({
	Name = "Tab 1",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

MainTab:AddButton({
	Name = "Pet Equip buy",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/Darks-scripts/pet-equip-buy/main/pet"))()
  	end    
})


MainTab:AddToggle({
	Name = "Next world auto buy",
	Default = true,
	Callback = function(Value)
		task.wait(88)
                local args = {
    [1] = "Colorful Forest"
}

game:GetService("ReplicatedStorage").Network.Zones_RequestPurchase:InvokeServer(unpack(args))
	end    
})


